{
  "name": "light-control",
  "version": "1.0.0",
  "description": "React app that controls lights in the house",
  "main": "server/index.js",
  "scripts": {
    "build": "webpack",
    "start": "node server/index.js",
    "linter": "eslint app/**/*.js app/**/*.jsx server/**/*.js webpack.*.js",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "verbose": true,
    "moduleDirectories": [
      "node_modules"
    ],
    "transformIgnorePatterns": [
      "node_modules/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YashdalfTheGray/light-control.git"
  },
  "keywords": [
    "react",
    "hue-remote",
    "iot",
    "lights"
  ],
  "author": "Yash Kulshrestha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YashdalfTheGray/light-control/issues"
  },
  "homepage": "https://github.com/YashdalfTheGray/light-control#readme",
  "dependencies": {
    "body-parser": "~1.17.2",
    "chalk": "~1.1.3",
    "dotenv": "~4.0.0",
    "express": "~4.15.3",
    "jsonwebtoken": "~7.4.1",
    "lodash": "~4.17.4",
    "material-ui": "~0.18.3",
    "mongodb": "~2.2.29",
    "morgan": "~1.8.2",
    "prop-types": "~15.5.10",
    "react": "~15.6.1",
    "react-dom": "~15.6.1",
    "react-router-dom": "~4.1.1",
    "react-tap-event-plugin": "~2.0.1",
    "react-transition-group": "~1.2.0",
    "redux": "~3.7.0",
    "redux-devtools-extension": "~2.13.2",
    "redux-saga": "~0.15.3",
    "request": "~2.81.0",
    "request-promise": "~4.2.1",
    "uuid": "~3.1.0"
  },
  "devDependencies": {
    "babel-core": "~6.25.0",
    "babel-eslint": "~7.2.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "~7.1.0",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "babel-plugin-transform-object-rest-spread": "~6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "~6.24.1",
    "babel-preset-react": "~6.24.1",
    "eslint": "~3.19.0",
    "eslint-config-airbnb": "~15.0.1",
    "eslint-plugin-import": "~2.5.0",
    "eslint-plugin-jsx-a11y": "~5.0.3",
    "eslint-plugin-react": "~7.1.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "redux-saga-test-plan": "^3.3.0",
    "regenerator-runtime": "^0.11.0",
    "webpack": "~3.0.0"
  }
